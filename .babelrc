{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "> 3% in US",
            "last 5 Chrome versions",
            "last 5 Firefox versions",
            "last 5 Edge versions",
            "ie >= 9"
          ]
        }
      }
    ]
  ],
  "compact": "true",
  "minified": "true",
  "plugins": [
    "transform-object-rest-spread",
    "lodash",
    "transform-runtime"
  ]
}
